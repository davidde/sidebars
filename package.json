{
  "name": "sidebars",
  "description": "Simple responsive sidebars",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "sidebar",
    "react",
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
